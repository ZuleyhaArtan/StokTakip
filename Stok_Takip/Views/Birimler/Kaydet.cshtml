@model Stok_Takip.Models.Entity.Birimler
@{
    ViewBag.Title = "Ekle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Birim Kaydetme</h2>
<div class="container">
    @using (Html.BeginForm("Kaydet", "Birimler", FormMethod.Post))
    {
    <div class="form-group">
        @Html.HiddenFor(x=>x.ID)
        @Html.LabelFor(x => x.Birimler1)
        @Html.TextBoxFor(x => x.Birimler1,new { @class = "form-control" } )

    </div>
    <div class="form-group">
         
      <button class="btn btn-success">Birim Ekle</button>

    </div>
    }


</div>

